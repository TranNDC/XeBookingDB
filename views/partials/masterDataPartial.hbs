<div class="container-fluid  text-center d-flex flex-column my-table b3-table" id="search-table-result">
        <div class="row table-header big-label">
                <div class="col-2 active-filter"><a class="action-sm" href="#">
                                <i class="far fa-circle"></i>
                        </a> Departure <i class="fas fa-arrow-down"></i></div>
                <div class="col-2">Minute moving</div>
                <div class="col-2">Bus Route</div>
                <div class="col-2">Bus ID</div>
                <div class="col-2">Price</div>
                <div class="col-2">Action</div>
        </div>
        <div class="b3-content-containter">
                {{#each results}}

                <div class="row table-content">
                        <div class="col-2  active-filter"><a class="action" href="#">
                                        <i class="far fa-circle"></i>
                                </a>
                                {{prettifyDate ngayGioKhoiHanh}}<br>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;{{getTime ngayGioKhoiHanh}}
                        </div>
                        <div class="col-2">{{Tuyen.soPhutDiChuyen}}</div>
                        <div class="col-2">{{Tuyen.xuatphat.ten}}-{{Tuyen.ketthuc.ten}}</div>
                        <div class="col-2">{{Xe.bienso}}</div>
                        <div class="col-2">{{gia}}</div>
                        <div class="col-2">
                                <a class="action" href="#">
                                        <i class="fas fa-eye"></i>
                                </a>
                                <a class="action" href="#">
                                        <i class="fas fa-pen"></i>
                                </a>
                                <a class="action" href="#">
                                        <i class="fas fa-trash"></i>
                                </a>
                        </div>
                </div>

                <hr>
                {{/each}}
        </div>


        <nav class="d-flex">
                {{{paginateHelper pagination}}}
        </nav>
</div>