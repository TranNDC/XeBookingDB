<div class="container-fluid  text-center d-flex flex-column my-table b3-table" id="search-table-result">
        <div class="row table-header big-label">
                <div class="col-2"><a id="headervoucher" class="listHeader" href="?order=voucher_desc">Voucher</a>
                </div>
                <div class="col-2"><a id="headerbusroute" class="listHeader" href="?order=busroute_asc">Bus route</a></div>
                <div class="col-2"><a id="headertimestart" class="listHeader" href="?order=timestart_asc">Time start</a>
                </div>
                <div class="col-2"><a id="headertimeend" class="listHeader" href="?order=timeend_asc">Time end</a></div>
                <div class="col-2"><a id="headerdiscount" class="listHeader" href="?order=discount_asc">Discount</a></div>
                <div class="col-2">Action</div>
        </div>
        <div class="b3-content-containter">
                {{#each results}}

                <div class="row table-content">
                        <div class="voucherData col-2  active-filter">
                                {{maKhuyenMai}}
                        </div>
                        <div class="busRouteData col-2">{{Tuyen.xuatphat.ten}}-{{Tuyen.ketthuc.ten}}</div>
                        <div class="timeStartData col-2">{{prettifyDate ngayBatDau}}</div>
                        <div class="timeEndData col-2">{{prettifyDate ngayKetThuc}}</div>
                        <div class="discountData col-2">{{phanTram}}%</div>
                        <div class="col-2">
                                <a class="cursor-pointer action" data-toggle="modal" data-target="#modal{{@index}}">
                                        <i class="fas fa-eye"></i>
                                </a>
                                <a class="cursor-pointer action" href="/users/voucherdata/edit/{{id}}">
                                        <i class="fas fa-pen" style="color:#26292D;"></i>
                                </a>
                                <a class="cursor-pointer action" data-toggle="modal" data-target="#yesnoModal">
                                        <i class="fas fa-trash"></i>
                                </a>
                        </div>
                </div>

                <hr>

                <!-- YesnoModal -->
                <div class="modal fade" id="yesnoModal" tabindex="-1" role="dialog"
                        aria-labelledby="exampleModalLabel" aria-hidden="true">
                        <div class="modal-dialog" role="document">
                                <div class="modal-content">
                                        <div class="modal-header">
                                                <h5 class="modal-title" id="exampleModalLabel">Warning</h5>
                                                <button type="button" class="close" data-dismiss="modal"
                                                        aria-label="Close">
                                                        <span aria-hidden="true">&times;</span>
                                                </button>
                                        </div>
                                        <div class="modal-body">
                                                Do you want to delete this departure?
                                        </div>
                                        <div class="modal-footer">
                                                <button type="button" class="btn btn-primary btn-dark"
                                                        data-dismiss="modal">No</button>
                                                <a class="btn btn-primary" href="/users/voucherdata/delete/{{id}}">Yes</a>
                                        </div>
                                </div>
                        </div>
                </div>


                <div class="modal fade" id="modal{{@index}}">
                        <div class="modal-dialog modal-lg">
                                <div class="modal-content">

                                        <!-- Modal Header -->
                                        <div class="modal-header pb-0">
                                                <button type="button" class="close"
                                                        data-dismiss="modal">&times;</button>
                                        </div>
                                        <ul class="nav nav-tabs pb-4 pt-0 d-flex align-items-end mb-3" role="tablist"
                                                style="margin:0 15%">
                                                <li class="nav-item">
                                                        <a id="ahome" class="nav-link a2-nav-link active"
                                                                data-toggle="tab"
                                                                href="#modalGeneral{{@index}}">General</a>
                                                </li>
                                                <li class="nav-item">
                                                        <a id="amenu1" class="nav-link a2-nav-link" data-toggle="tab"
                                                                href="#modalPassengerContact{{@index}}">Other
                                                                details</a>
                                                </li>
                                        </ul>


                                        <!-- Modal body -->
                                        <div class="modal-body   pl-5 pb-5 pr-5">
                                                <div class="tab-content text-left">
                                                        <!-- General Information -->
                                                        <div id="modalGeneral{{@index}}"
                                                                class="container tab-pane active">
                                                                <div class="row">
                                                                        <div class="col-lg-3 col-sm-4 lighter-text">
                                                                                Voucher ID</div>
                                                                        <div class="col-lg-3 col-sm-6">{{id}}</div>

                                                                        <div class="col-lg-3 col-sm-4 lighter-text">
                                                                                Created at
                                                                        </div>
                                                                        <div class="col-lg-3 col-sm-6">{{prettifyDate createdAt}}
                                                                                - {{getTimeC createdAt}} </div>
                                                                </div>
                                                                <hr>
                                                                
                                                                <div class="row">
                                                                        <div class="col-lg-3 col-sm-4 lighter-text">
                                                                                Voucher
                                                                        </div>
                                                                        <div class="col-lg-3 col-sm-6">
                                                                                {{maKhuyenMai}}
                                                                        </div>

                                                                        <div class="col-lg-3 col-sm-4 lighter-text">
                                                                                Discount
                                                                        </div>
                                                                        <div class="col-lg-3 col-sm-6">
                                                                                {{phanTram}}%
                                                                        </div>
                                                                </div>
                                                                <div class="row">
                                                                        <div class="col-lg-3 col-sm-4 lighter-text">
                                                                                Time start
                                                                        </div>
                                                                        <div class="col-lg-3 col-sm-6">
                                                                            {{prettifyDate ngayBatDau}} - {{getTimeC ngayBatDau}}
                                                                        </div>

                                                                        <div class="col-lg-3 col-sm-4 lighter-text">
                                                                                Time end
                                                                        </div>
                                                                        <div class="col-lg-3 col-sm-6">
                                                                            {{prettifyDate ngayKetThuc}} - {{getTimeC ngayKetThuc}}
                                                                                </div>
                                                                </div>
                                                                <hr>

                                                                <div class="row">

                                                                        <div class="col-lg-3 col-sm-4 lighter-text">
                                                                                Bus route
                                                                        </div>
                                                                        <div class="col-lg-3 col-sm-6">
                                                                            {{Tuyen.xuatphat.ten}} - {{Tuyen.ketthuc.ten}}
                                                                        </div>
                                                                      

                                                                </div>
                                                                
                                                                <hr>
                                                                <div class="row">
                                                                        <div class="col-lg-3 col-sm-4">
                                                                                <img  src="{{imagePath}}" alt="picVoucher"
                                                                                height="500" width="675"
                                                                                style="margin-left: -25%">
                                                                        </div>
                                                                    

                                                                </div>
                                                            
                                                        </div>

                                                        <div id="modalPassengerContact{{@index}}"
                                                                class="container tab-pane">
                                                                <div class="row">
                                                                        <div class="col-lg-3 col-sm-4 lighter-text">
                                                                                Departure station
                                                                        </div>
                                                                        <div class="col-lg-3 col-sm-6">
                                                                                {{Tuyen.xuatphat.ten}}</div>
                                                                        <div class="col-lg-3 col-sm-4 lighter-text">
                                                                                Arrival station
                                                                        </div>
                                                                        <div class="col-lg-3 col-sm-6">
                                                                                {{Tuyen.ketthuc.ten}}</div>
                                                                </div>
                                                                <hr>
                                                                <div class="row">
                                                                        <div class="col-lg-3 col-sm-4 lighter-text">
                                                                                Departure address
                                                                        </div>
                                                                        <div class="col-lg-3 col-sm-6">
                                                                                {{Tuyen.xuatphat.diachi}}</div>
                                                                        <div class="col-lg-3 col-sm-4 lighter-text">
                                                                                Arrival address
                                                                        </div>
                                                                        <div class="col-lg-3 col-sm-6">
                                                                                {{Tuyen.ketthuc.diachi}}</div>


                                                                </div>
                                                                <hr>
                                                                <div class="row">
                                                                        <div class="col-lg-3 col-sm-4 lighter-text">
                                                                                Departure phone
                                                                        </div>
                                                                        <div class="col-lg-3 col-sm-6">
                                                                                {{Tuyen.xuatphat.sdt}}</div>

                                                                        <div class="col-lg-3 col-sm-4 lighter-text">
                                                                                Arrival phone
                                                                        </div>
                                                                        <div class="col-lg-3 col-sm-6">
                                                                                {{Tuyen.ketthuc.sdt}}</div>
                                                                </div>
                                                                <hr>

                                                        </div>
                                                </div>
                                        </div>

                                </div>
                        </div>
                </div>

                {{/each}}
        </div>


        <nav class="d-flex">
                {{#if hasPagination}}
                {{{paginate pagination}}}
                {{/if}}
        </nav>
</div>